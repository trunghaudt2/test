{
  "@context": "https://schema.org/",
  "@type": "Product",
  "name": "{{ page.title }}",
  "image": ["https://cualuoidanang.com{{ page.image }}"],
  "description": "{{ page.excerpt | strip_html }}",
  "sku": "{{ page.sku }}",
  "mpn": "{{ page.sku }}",
  "brand": {
    "@type": "Thing",
    "name": "Sinh Tài"
  },
  "review": {
    "@type": "Review",
    "reviewRating": {
      "@type": "Rating",
      "ratingValue": "5",
      "bestRating": "5"
    },
    "author": {
      "@type": "Person",
      "name": "A.Thành"
    }
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "5.0",
    "reviewCount": "89"
  },
  "offers": {
    "@type": "Offer",
    "url": "https://cualuoidanang.com/{{ page.url }}",
    "priceCurrency": "VND",
    "price": "{{ page.price }}",
    "priceValidUntil": "2020-11-25",
    "itemCondition": "https://schema.org/UsedCondition",
    "availability": "https://schema.org/InStock",
    "seller": {
      "@type": "Organization",
      "name": "{{ site.name }}"
    }
  }
}